option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: shout_app/wsgi.py

commands: 
    01_copy_language_model:
        command:
            - chown 777 -R /opt/python/current/app
            - aws s3 cp --recursive s3://sentencemodel/language_model /opt/python/current/app
            - chown 400 -R /opt/python/current/app
