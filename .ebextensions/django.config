option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: shout_app/wsgi.py

commands: 
    01_change_my_folder_permissions:
    command: "chmod 777 -R /opt/python/current/app"

    02_copy_language_model:
    command: aws s3 cp --recursive s3://sentencemodel/language_model /opt/python/current/app
